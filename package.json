  "dependencies": {
    "babel-loader": "^8.0.6",
    "bulma": "^0.8.2",
    "concurrently": "^5.2.0"
  },
  "scripts": {
    "build": "npm run css-deploy &&  webpack -p --env.API_URL=https://www.facteurs-charge.fr --progress --config static/webpack.config.js",
    "dev-build": "npm run css-deploy && webpack --env.API_URL=http://localhost:8080 --progress -d --config static/webpack.config.js",
    "watch": "concurrently \"npm run start\" \"npm run css-watch\" \"webpack --env.API_URL=http://localhost:8080 --progress -d --config static/webpack.config.js --watch\"",
    "start": "nodemon server.js",
    "css-deploy": "npm run css-build && npm run css-postcss",
    "css-build": "node-sass static/_sass/main.scss static/css/main.css",
    "css-postcss": "postcss --use autoprefixer --output static/css/main.css static/css/main.css",
    "css-watch": "npm run css-build -- --watch"
  }
}
